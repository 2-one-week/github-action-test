
{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "allowJs": true,
      "noEmit": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
    }
}